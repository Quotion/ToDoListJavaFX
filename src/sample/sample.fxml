<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="but1" layoutX="409.0" layoutY="17.0" mnemonicParsing="false" onAction="#onButtonClick" text="Button" />
      <TextField fx:id="fieldForTodo" layoutX="484.0" layoutY="17.0" promptText="Введите пункт" />
      <VBox fx:id="vboxTodo" layoutX="25.0" layoutY="20.0" prefHeight="369.0" prefWidth="370.0" />
      <ChoiceBox fx:id="choiceBox" accessibleText="Куку" layoutX="409.0" layoutY="63.0" prefWidth="150.0" AnchorPane.rightAnchor="92.0" />
      <TextField fx:id="categoryEntry" layoutX="409.0" layoutY="106.0" onKeyPressed="#createCategory" prefHeight="27.0" prefWidth="150.0" promptText="Название категории" visible="false" />
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>
